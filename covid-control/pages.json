{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "个人健康管理小程序",
				"enablePullDownRefresh": true,
				"navigationBarBackgroundColor": "#E4FFFA"
			}
		},
		{
			"path": "pages/detect/index",
			"style": {
				"navigationBarTitleText": "感染预测",
				"enablePullDownRefresh": true,
				"navigationBarBackgroundColor": "#E4FFFA"
			}
		},
		{
			"path": "pages/prediction/index",
			"style": {
				"navigationBarTitleText": "康复监测",
				"enablePullDownRefresh": true,
				"navigationBarBackgroundColor": "#E4FFFA"
			}
		},
		{
			"path": "pages/myinfo/index",
			"style": {
				"navigationBarTitleText": "我的页面",
				"navigationBarBackgroundColor": "#E4FFFA"
			}
		}
	    ,{
            "path" : "pages/predictionresult/predictionresult",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "预测结果",
                "enablePullDownRefresh": true,
				"navigationBarBackgroundColor": "#E4FFFA"
            }
            
        }
        ,{
            "path" : "pages/recoveryresult/recoveryresult",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "康复日程",
                "enablePullDownRefresh": true,
				"navigationBarBackgroundColor": "#E4FFFA"
            }
            
        }
        ,{
            "path" : "pages/recovery/recovery",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "康复手册",
                "enablePullDownRefresh": true,
				"navigationBarBackgroundColor": "#E4FFFA"
            }
            
        },
		{
		    "path" : "pages/recoverymanual/tipSign",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "风险提醒",
		        "enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#E4FFFA"
		    }
		    
		}
		
    ],
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/tabBar/首页.png",
			"selectedIconPath": "static/tabBar/首页1.png",
			"text": "首页"
		}, 
		{
			"pagePath": "pages/myinfo/index",
			"iconPath": "static/tabBar/我的.png",
			"selectedIconPath": "static/tabBar/我的-filled.png",
			"text": "我的"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "感染症状监测",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}
